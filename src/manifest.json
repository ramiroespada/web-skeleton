{
  "manifest_version": 3,
  "name": "Web Skeleton",
	"version": "0.2",
	"description" :"Decorate DOM elements to have visual feedback of the skeleton.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
	"icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
	"background": {
    "service_worker": "background.js"
  },
	"action": {
    "default_title": "Toggle Web Skeleton"
  }
}
